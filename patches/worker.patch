- op: add
  path: /machine/network
  value:
    interfaces:
    - interface: eth0
      dhcp: false
      addresses: 
        - ${IPADDRESS}
      routes:
        - network: 0.0.0.0/0
          gateway: ${GATEWAY}
- op: replace #Cilium CNI
  path: /cluster/network/cni
  value:
    name: "custom"
    urls:
      - "https://github.com/RobM83/talos-vmware/blob/9aa5f55677067c1e02abd4704311e13629df3403/cilium/cilium.yaml"
